generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User{
  Id        Int       @id         @default(autoincrement())
  Email     String    @unique
  Password  String
  History   Test[]    
}


model Test{
  Id        Int       @id         @default(autoincrement())
  UserId    Int
  User      User @relation(fields: [UserId], references: [Id])
}